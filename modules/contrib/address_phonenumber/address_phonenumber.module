<?php

/**
 * @file
 * Provides address phonenumber field.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function address_phonenumber_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.address_phonenumber':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('The Address Phonenumber module enhances the Address module by providing phonenumber field.') . '</p>';
      return $output;
  }
}
